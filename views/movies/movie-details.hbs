
<h1>Movie Detail:</h1>
<p>Title: {{title}}</p>
<p>Genre: {{genre}}</p>
<p>Plot: {{plot}}</p>
<p>Cast:</p>

{{#each cast}}
<p>Name: {{this.name}}</p>
<p>Occupation: {{this.occupation}}</p>
<p>Catchphrase: {{this.catchPhrase}}</p>
{{/each}}

<form action="/movies/{{this._id}}/delete" method="post">
<button type="submit">Delete</button>
</form>
<a href="/movies/{{this._id}}/edit">Edit</a>

